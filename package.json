{
  "name": "WebRTC-Example",
  "version": "1.0.0",
  "description": "An 'as simple as it gets' WebRTC example.",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "test": "node server/server.js -env local",
    "webgl": "node server/server.js -env local -v webgl",
    "frame": "node server/server.js -env local -v frame",
    "photo3d": "node server/server.js -env local -p 8602",
    "none": "node server/server.js -env local -p 8600",
    "profile": "node server/server.js -env local -p 8601",
    "eye0": "node server/server.js --viz=none --port=8444",
    "eye1": "node server/server.js --viz=frame --port=8445",
    "eye2": "node server/server.js --viz=webgl--port=8446",
    "eye3": "node server/server.js --viz=profile--port=8447",
    "chat0": "node server/server.js --port=8600",
    "chat1": "node server/server.js --port=8601",
    "chat2": "node server/server.js --port=8602"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanet/WebRTC-Example.git"
  },
  "keywords": [
    "webrtc"
  ],
  "author": "shane tully",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shanet/WebRTC-Example/issues"
  },
  "homepage": "https://github.com/shanet/WebRTC-Example#readme",
  "dependencies": {
    "ws": "^3.3.2",
    "yargs": "^15.4.0"
  }
}
